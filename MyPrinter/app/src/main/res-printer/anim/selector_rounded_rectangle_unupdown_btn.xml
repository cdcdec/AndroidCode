<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#fffafafa" android:endColor="#fffafafa" android:angle="0.0" android:centerColor="#fffafafa" />
            <stroke android:width="0.5dip" android:color="#ffbbbbbb" />
            <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
        </shape>
    </item>
    <item>
        <shape android:state_focused="false">
            <gradient android:startColor="#ff9fc5ff" android:endColor="#ff9fc5ff" android:angle="0.0" android:centerColor="#ff9fc5ff" />
            <stroke android:width="0.5dip" android:color="#ffbbbbbb" />
            <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
        </shape>
    </item>
</selector>