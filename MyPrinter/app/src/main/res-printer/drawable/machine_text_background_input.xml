<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="4.0px" />
            <solid android:color="#ffffffff" />
            <stroke android:width="2.0px" android:color="#ff0066cc" />
            <padding android:left="5.0px" android:top="8.0px" android:right="5.0px" android:bottom="8.0px" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <corners android:radius="4.0px" />
            <solid android:color="#ffffffff" />
            <stroke android:width="2.0px" android:color="#ff0066cc" />
            <padding android:left="5.0px" android:top="8.0px" android:right="5.0px" android:bottom="8.0px" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:radius="4.0px" />
            <solid android:color="#ffffffff" />
            <stroke android:width="2.0px" android:color="#ff0066cc" />
            <padding android:left="5.0px" android:top="8.0px" android:right="5.0px" android:bottom="8.0px" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#ffbdc3c7" />
            <stroke android:width="2.0px" />
            <corners android:radius="4.0px" />
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="135.0" />
            <stroke android:width="2.0px" android:color="#ffbdc3c7" android:dashWidth="0.0px" android:dashGap="0.0px" />
            <padding android:left="5.0px" android:top="8.0px" android:right="5.0px" android:bottom="8.0px" />
        </shape>
    </item>
</selector>