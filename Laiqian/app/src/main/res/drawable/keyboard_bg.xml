<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#fff8f8f8" android:angle="180.0" />
            <solid android:color="@color/viewgroup_pressed" />
            <stroke android:width="1.0dip" android:color="@color/ui201404_smj_viewgroup_background_white_stroke_2corners" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#fff8f8f8" android:angle="180.0" />
            <solid android:color="@color/ui201404_smj_viewgroup_background_white_solid" />
            <stroke android:width="1.0dip" android:color="#80c0bfbe" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#fff8f8f8" android:angle="180.0" />
            <solid android:color="@color/pos_big_b_color_10500" />
            <stroke android:width="1.0dip" android:color="@color/ui201404_smj_viewgroup_background_white_stroke_2corners" />
        </shape>
    </item>
</selector>