<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <LinearLayout
           style="@style/lin1">

        <Button
                android:id="@+id/btnAbout"
                style="@style/btn1"
                android:text="关于"/>

        <Button
            android:id="@+id/btnAgreement"
            style="@style/btn1"
            android:text="用户协议"/>

        <Button
                android:id="@+id/btnAliOrder"
                style="@style/btn1"
                android:text="aliOrder"/>

        <Button
                android:id="@+id/btnWelcome"
                style="@style/btn1"
                android:text="欢迎页面"/>
        <Button
                android:id="@+id/btnBlueToothPrinterEdit"
                style="@style/btn1"
                android:text="蓝牙打印编辑"/>

        <Button
                android:id="@+id/btnLogin"
                style="@style/btn1"
                android:text="登录"/>
        <Button
                android:id="@+id/btnAfterLogin"
                style="@style/btn1"
                android:text="pos_downloader_after_login"/>

        <Button
                android:id="@+id/btnAppIntro"
                style="@style/btn1"
                android:text="app intro"/>

        <Button
                android:id="@+id/btnOrderOperation"
                style="@style/btn1"
                android:text="orderOperation"/>

    </LinearLayout>

</androidx.core.widget.NestedScrollView>