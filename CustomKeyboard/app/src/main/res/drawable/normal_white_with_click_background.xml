<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <stroke android:width="1.0dip" android:color="@color/setting_line_color" />
            <solid android:color="@color/viewgroup_pressed" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <stroke android:width="1.0dip" android:color="@color/setting_line_color" />
            <solid android:color="@color/form_color_bg" />
        </shape>
    </item>
</selector>