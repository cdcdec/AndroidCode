<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/ui201404_smj_viewgroup_background_white_pressed_solid" />
            <corners android:radius="4.0px" />
            <stroke android:width="1.0px" android:color="@color/titlebar_background_color" />
        </shape>
    </item>
    <item android:state_checked="true" android:state_pressed="false">
        <shape>
            <solid android:color="@color/ui201404_smj_viewgroup_background_white_solid" />
            <corners android:radius="4.0px" />
            <stroke android:width="1.0px" android:color="@color/titlebar_background_color" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/ui201404_smj_viewgroup_background_white_pressed_solid" />
            <corners android:radius="4.0px" />
            <stroke android:width="1.0px" android:color="@color/ui201404_smj_viewgroup_background_white_stroke" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/ui201404_smj_viewgroup_background_white_solid" />
            <corners android:radius="4.0px" />
            <stroke android:width="1.0px" android:color="@color/ui201404_smj_viewgroup_background_white_stroke" />
        </shape>
    </item>
</selector>