<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="4.0px" />
            <gradient android:startColor="#ffcbcbcb" android:endColor="#ffcbcbcb" android:angle="90.0" />
        </shape>
    </item>
    <item android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="2.0px">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffd4e2f1" android:endColor="#ffd4e2f1" android:angle="90.0" />
            <stroke android:width="3.0px" android:color="#ffffffff" />
            <corners android:radius="5.0px" />
        </shape>
    </item>
</layer-list>