<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="4.0px" />
            <gradient android:startColor="#ffd9ccc1" android:endColor="#ffd9ccc1" android:angle="90.0" />
        </shape>
    </item>
    <item android:state_pressed="true" android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="2.0px">
        <shape>
            <gradient android:startColor="#ffb89e8b" android:endColor="#ffb89e8b" android:angle="180.0" />
            <corners android:radius="4.0px" />
            <padding android:left="10.0px" android:top="10.0px" android:right="10.0px" android:bottom="10.0px" />
        </shape>
    </item>
</layer-list>