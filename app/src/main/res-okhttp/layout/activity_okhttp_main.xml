<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".libraries.okhttp.OkHttpMainActivity">
    <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:dividerPadding="10dp"
            app:divider="@drawable/shape_line1"
            app:showDividers="middle|end"
    >
        <Button
                android:id="@+id/btnOkHttpGet"
                style="@style/comBtnStyle1"
                android:text="@string/okHttpGet"/>

        <Button android:id="@+id/btnOkHttpPost"
                style="@style/comBtnStyle1"
                android:text="@string/okHttpPost"/>


        <Button
                android:id="@+id/btnOkHttpFile"
                style="@style/comBtnStyle1"
                android:text="@string/okHttpFile"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.core.widget.NestedScrollView>