        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".libraries.camera.CameraMainActivity">

    <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:divider="@drawable/shape_line1"
            android:orientation="vertical">
        <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/camera1"
                style="@style/comBtnStyle1"
                android:text="camera1\n\https://github.com/Lee465357793/CameraORC"
        />

        <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/camera2"
                style="@style/comBtnStyle1"
                android:text="camera2"
        />




    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.core.widget.NestedScrollView>