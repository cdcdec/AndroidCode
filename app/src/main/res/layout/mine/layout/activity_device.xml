<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:divider="@drawable/shape_line1"
        app:showDividers="middle|end"
        app:dividerPadding="10dp"
        tools:context=".mine.DeviceActivity">
    <TextView
            android:id="@+id/tvVersion"
            style="@style/comTvStyle1"
            android:text="@string/sdk_version_name"
    />
    <TextView
            android:id="@+id/tvVersionSdk"
            style="@style/comTvStyle1"
            android:text="@string/sdk_version_code"
    />
    <TextView
            android:id="@+id/tvAndroidId"
            style="@style/comTvStyle1"
            android:text="@string/android_id"
    />
    <TextView
            android:id="@+id/tvMacAds"
            style="@style/comTvStyle1"
            android:text="@string/mac_address"
    />

    <TextView
            android:id="@+id/tvManufacturer"
            style="@style/comTvStyle1"
            android:text="@string/manufacturer"
    />
    <TextView
            android:id="@+id/tvModel"
            style="@style/comTvStyle1"
            android:text="@string/model"
    />
    <TextView
            android:id="@+id/tvRoot"
            style="@style/comTvStyle1"
            android:text="@string/isRoot"
    />

    <TextView
            android:id="@+id/tvSmallWidth"
            style="@style/comTvStyle1"
            android:text="@string/smallWidth"
    />

    <TextView
            android:id="@+id/tvDensity"
            style="@style/comTvStyle1"
            android:text="@string/logical_density"
    />
    <TextView
            android:id="@+id/tvWidthHeight"
            style="@style/comTvStyle1"
            android:text="@string/width_height"
    />

    <TextView
            android:id="@+id/tvInch"
            style="@style/comTvStyle1"
            android:text="@string/width_height"
    />
</androidx.appcompat.widget.LinearLayoutCompat>