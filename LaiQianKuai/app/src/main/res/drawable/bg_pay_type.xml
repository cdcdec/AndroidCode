<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/red_color_10500" />
            <corners android:radius="2.0dip" />
            <stroke android:width="1.0px" android:color="@color/setting_line_color" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/setting_select_bg_color" />
            <corners android:radius="2.0dip" />
            <stroke android:width="1.0px" android:color="@color/setting_line_color" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/transparent" />
            <corners android:radius="2.0dip" />
            <stroke android:width="1.0px" android:color="@color/setting_line_color" />
        </shape>
    </item>
</selector>