            <gradient android:startColor="#ffaecaf2" android:endColor="#ffaecaf2" android:angle="270.0" />
            <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="4.0dip" android:bottomRightRadius="0.0dip" />
