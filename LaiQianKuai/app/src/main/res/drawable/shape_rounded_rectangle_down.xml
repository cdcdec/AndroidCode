<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape>
                    <corners android:bottomLeftRadius="@dimen/ui201404_smj_viewgroup_background_white_corners" android:bottomRightRadius="@dimen/ui201404_smj_viewgroup_background_white_corners" />
                    <solid android:color="@color/ui201404_smj_viewgroup_background_white_solid" />
                    <stroke android:width="1.0px" android:color="@color/ui201404_smj_viewgroup_background_white_stroke_2corners" />
                </shape>
            </item>
            <item android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="1.0px">
                <shape>
                    <solid android:color="@color/ui201404_smj_viewgroup_background_white_solid" />
                    <corners android:bottomLeftRadius="@dimen/ui201404_smj_viewgroup_background_white_corners" android:bottomRightRadius="@dimen/ui201404_smj_viewgroup_background_white_corners" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/viewgroup_pressed" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/pos_big_b_color_10500" />
        </shape>
    </item>
</selector>