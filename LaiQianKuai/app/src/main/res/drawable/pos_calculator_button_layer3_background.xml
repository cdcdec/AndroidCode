<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="0.0px" />
            <gradient android:startColor="#ff34495e" android:endColor="#ff34495e" android:angle="90.0" />
        </shape>
    </item>
    <item android:left="2.0px">
        <shape android:shape="rectangle">
            <stroke android:width="1.0px" android:color="#ffbdc3c7" android:bottom="2.0px" />
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="90.0" />
            <corners android:radius="0.0px" />
        </shape>
    </item>
</layer-list>