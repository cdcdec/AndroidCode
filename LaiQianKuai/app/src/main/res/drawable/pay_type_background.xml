<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <solid android:color="@color/white_color_1033" />
            <stroke android:width="1.0dip" android:color="@color/white_color_1033" />
        </shape>
    </item>
    <item android:state_activated="true">
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <solid android:color="@color/white_color_1033" />
            <stroke android:width="1.0dip" android:color="@color/white_color_1033" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <solid android:color="@color/setting_line_color" />
            <stroke android:width="1.0dip" android:color="@color/setting_line_color" />
        </shape>
    </item>
</selector>